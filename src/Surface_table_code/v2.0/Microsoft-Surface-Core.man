<?xml version="1.0" encoding="utf-8"?>
<instrumentationManifest xmlns="http://schemas.microsoft.com/win/2004/08/events">
  <instrumentation xmlns:xs="http://www.w3.org/2001/XMLSchema"
                   xmlns:win="http://manifests.microsoft.com/win/2004/08/windows/events"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <events xmlns="http://schemas.microsoft.com/win/2004/08/events">
      <provider xmlns:mtc="http://schemas.microsoft.com/surface/2007/09/manifesttocode"
                mtc:providerNamespace="Microsoft.Surface.Core"
                mtc:internal="true"
                name="Microsoft-Surface-Core" guid="{88931919-5519-47EB-8BD9-DF363E0DB6CB}"
                symbol="EventsCore"
                resourceFileName="%ProgramFiles%\Microsoft Surface\v2.0\Microsoft-Surface-Core.man.dll"
                messageFileName="%ProgramFiles%\Microsoft Surface\v2.0\Microsoft-Surface-Core.man.dll"
                parameterFileName="%ProgramFiles%\Microsoft Surface\v2.0\Microsoft-Surface-Core.man.dll" >
        <channels>
          <!-- This channel is used to publish events destined to the system administrator -->
          <channel name="Microsoft-Surface-Core/Operational" chid="channelOperational"
                   symbol="channelOperational" type="Operational" isolation="Application" enabled="true" value="16" />
          <!-- This channel is used to publish events used in performance tracing -->
          <channel name="Microsoft-Surface-Core/Debug" chid="channelDebug"
                   symbol="channelDebug" type="Debug" isolation="Application" enabled="false" value="17" />
          <!-- This channel is used to publish events used in debug tracing -->
          <channel name="Microsoft-Surface-Core/Performance" chid="channelPerformance"
                   symbol="channelPerformance" type="Analytic" isolation="Application" enabled="false" value="18" />
          <!-- Add new channels above this line -->
        </channels>
        
        <templates>
          <template tid="templateNone">
          </template>

          <template tid="templateId">
            <data name="id" inType="win:Int32" />
            <UserData>
              <integerData xmlns="nullNS">
                <id>%1</id>
            </integerData>
            </UserData>
          </template>

          <template tid="templateException">
            <data name="exception" inType="win:UnicodeString" />
            <UserData>
              <stringData xmlns="nullNS">
                <exception>%1</exception>
            </stringData>
            </UserData>
          </template>

          <template tid="templatePerformanceCounterException">
            <data name="name" inType="win:UnicodeString" />
            <data name="exception" inType="win:UnicodeString" />
            <UserData>
              <stringData xmlns="nullNS">
                <name>%1</name>
                <exception>%2</exception>
              </stringData>
            </UserData>
          </template>

          <template tid="templateHashCodeHwnd">
            <data name="hashCode" inType="win:Int32" />
            <data name="hwnd" inType="win:Int32" />
            <UserData>
              <integerData xmlns="nullNS">
                <hashCode>%1</hashCode>
                <hwnd>%2</hwnd>
              </integerData>
            </UserData>
          </template>
          
          <template tid="templateTouchEventDebug">
            <data name="hashCode" inType="win:Int32" />
            <data name="id" inType="win:Int32" />
            <data name="x" inType="win:Double" />
            <data name="y" inType="win:Double" />
            <UserData>
              <data xmlns="nullNS">
                <hashCode>%1</hashCode>
                <id>%2</id>
                <x>%3</x>
                <y>%4</y>
              </data>
            </UserData>
          </template>
          
          <!-- Add new templates above this line -->
        </templates>
        
        <opcodes>
          <!-- Add new opcodes above this line -->
        </opcodes>

        <keywords>
          <keyword name="KeywordOperational" mask="0x00000001" message="$(string.KeywordOperational)" symbol="KeywordOperational" />
          <keyword name="KeywordDebug" mask="0x000000002" message="$(string.KeywordDebug)" symbol="KeywordDebug" />
          <keyword name="KeywordPerformance" mask="0x00000004" message="$(string.KeywordPerformance)" symbol="KeywordPerformance" />
          <!-- Add new keywords above this line (0x000000000001 to 0x800000000000) -->
        </keywords>
        
        <tasks>
          <task name="DebugTouchEvents" value="100" />
          <task name="TouchTarget" value="101" />
          <task name="InputLoop" value="102" />
          <task name="PerfTouchEvents" value="200" />
          <task name="ReadInputFrame" value="201" />
        </tasks>
        
        <events>
          <!-- ************************************************** -->
          <!-- START EVENTS PUBLISHED TO "Administration" channel -->
          <!-- ************************************************** -->
          <!-- ************************************************ -->
          <!-- END EVENTS PUBLISHED TO "Administration" channel -->
          <!-- ************************************************ -->

          <!-- *********************************************** -->
          <!-- START EVENTS PUBLISHED TO "Operational" channel -->
