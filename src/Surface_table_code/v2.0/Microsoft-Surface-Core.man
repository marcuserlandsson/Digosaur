          <event symbol="CannotConnectToVisionSystem" value="1000" level="win:Error" template="templateException"
               message="$(string.CannotConnectToVisionSystem)" channel="channelOperational" version="0" />
          <event symbol="UnexpectedExceptionDuringEventDispatching" value="1001" level="win:Error" template="templateException"
               message="$(string.UnexpectedExceptionDuringEventDispatching)" channel="channelOperational" version="0" />
          <event symbol="CannotInstantiatePerformanceCounter" value="1002" level="win:Error" template="templatePerformanceCounterException"
               message="$(string.CannotInstantiatePerformanceCounter)" channel="channelOperational" version="0" />
          <event symbol="PerformanceCountersAreNotInstalled" value="1003" level="win:Error" template="templateNone"
               message="$(string.PerformanceCountersAreNotInstalledError)" channel="channelOperational" version="0" />
          <!-- ********************************************* -->
          <!-- END EVENTS PUBLISHED TO "Operational" channel -->
          <!-- ********************************************* -->

          
          <!-- ***************************************** -->
          <!-- START EVENTS PUBLISHED TO "Debug" channel -->
          <!-- ***************************************** -->
          <event symbol="TouchDownDebug" value="1100" level="win:Informational"
                 keywords="KeywordDebug" task="DebugTouchEvents" opcode="win:Info"
                 template="templateTouchEventDebug" message="$(string.TouchDownDebug)" channel="channelDebug" version="0" />
          <event symbol="TouchMoveDebug" value="1101" level="win:Informational"
                 keywords="KeywordDebug" task="DebugTouchEvents" opcode="win:Info"
                 template="templateTouchEventDebug" message="$(string.TouchMoveDebug)" channel="channelDebug" version="0" />
          <event symbol="TouchUpDebug" value="1102" level="win:Informational"
                 keywords="KeywordDebug" task="DebugTouchEvents" opcode="win:Info"
                 template="templateTouchEventDebug" message="$(string.TouchUpDebug)" channel="channelDebug" version="0" />

          <event symbol="TouchTargetCreated" value="1110" level="win:Informational"
                 keywords="KeywordDebug" task="TouchTarget" opcode="win:Start"
                 template="templateHashCodeHwnd" message="$(string.TouchTargetCreated)" channel="channelDebug" version="0" />
          <event symbol="TouchTargetDisposed" value="1111" level="win:Informational"
                 keywords="KeywordDebug" task="TouchTarget" opcode="win:Stop"
                 template="templateHashCodeHwnd" message="$(string.TouchTargetDisposed)" channel="channelDebug" version="0" />

          <event symbol="InputLoopStart" value="1120" level="win:Informational"
                 keywords="KeywordDebug" task="InputLoop" opcode="win:Start"
                 template="templateNone" message="$(string.InputLoopStart)" channel="channelDebug" version="0" />
          <event symbol="InputLoopFinish" value="1121" level="win:Informational"
                 keywords="KeywordDebug" task="InputLoop" opcode="win:Stop"
                 template="templateNone" message="$(string.InputLoopFinish)" channel="channelDebug" version="0" />

          <!-- *************************************** -->
          <!-- END EVENTS PUBLISHED TO "Debug" channel -->
          <!-- *************************************** -->
          
          <!-- ******************************************************* -->
          <!-- START EVENT MESSAGES PUBLISHED TO "Performance" channel -->
          <!-- ******************************************************* -->
          <event symbol="RaiseTouchDown" value="1300" level="win:Informational"
                 keywords="KeywordPerformance" task="PerfTouchEvents" opcode="win:Info"
                 template="templateId" message="$(string.RaiseTouchDown)" channel="channelPerformance" version="0" />
          <event symbol="RaiseTouchMove" value="1301" level="win:Informational"
                 keywords="KeywordPerformance" task="PerfTouchEvents" opcode="win:Info"
                 template="templateId" message="$(string.RaiseTouchMove)" channel="channelPerformance" version="0" />
          <event symbol="RaiseTouchUp" value="1302" level="win:Informational"
                 keywords="KeywordPerformance" task="PerfTouchEvents" opcode="win:Info"
                 template="templateId" message="$(string.RaiseTouchUp)" channel="channelPerformance" version="0" />

          <event symbol="ReadTouchDown" value="1310" level="win:Informational"
                 keywords="KeywordPerformance" task="PerfTouchEvents" opcode="win:Info"
                 template="templateId" message="$(string.ReadTouchDown)" channel="channelPerformance" version="0" />
          <event symbol="ReadTouchMove" value="1311" level="win:Informational"
                 keywords="KeywordPerformance" task="PerfTouchEvents" opcode="win:Info"
                 template="templateId" message="$(string.ReadTouchMove)" channel="channelPerformance" version="0" />
          <event symbol="ReadTouchUp" value="1312" level="win:Informational"
                 keywords="KeywordPerformance" task="PerfTouchEvents" opcode="win:Info"
                 template="templateId" message="$(string.ReadTouchUp)" channel="channelPerformance" version="0" />

          <event symbol="ReadInputFrameStart" value="1320" level="win:Informational"
                 keywords="KeywordPerformance" task="ReadInputFrame" opcode="win:Start"
                 template="templateNone" message="$(string.ReadInputFrameStart)" channel="channelPerformance" version="0" />
          <event symbol="ReadInputFrameFinish" value="1321" level="win:Informational"
                 keywords="KeywordPerformance" task="ReadInputFrame" opcode="win:Stop"
                 template="templateNone" message="$(string.ReadInputFrameFinish)" channel="channelPerformance" version="0" />

          <event symbol="TouchTargetInitializeStart" value="1330" level="win:Informational"
                 keywords="KeywordPerformance" task="TouchTarget" opcode="win:Start"
                 template="templateNone" message="$(string.TouchTargetInitializeStart)" channel="channelPerformance" version="0" />
          <event symbol="TouchTargetInitializeFinish" value="1331" level="win:Informational"
                 keywords="KeywordPerformance" task="TouchTarget" opcode="win:Stop"
                 template="templateNone" message="$(string.TouchTargetInitializeFinish)" channel="channelPerformance" version="0" />



          <!-- ***************************************************** -->
          <!-- END EVENT MESSAGES PUBLISHED TO "Performance" channel -->
          <!-- ***************************************************** -->

        </events>
      </provider>
    </events>
  </instrumentation>
